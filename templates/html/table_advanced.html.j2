<html>
<head></head>
<body>

<h1>{{ items['dataset1'].name }}</h1>
Results: {{ items['dataset1'].summary.rows }}
<table>
<thead>
   <tr>
      {% for field in items['dataset1'].data[0] %}
      <TD class="c1">{{ field }}</TD>
      {% endfor %}
   </tr>
</thead>
{% for item in items['dataset1'].data %}
<tr>
   {% for field in item %}
   <TD class="c1">{{ item[field] }}</TD>
   {% endfor %}
</tr>
{% endfor %}
</table>
</table>
<script type="text/javascript">
   {%- for dataset in items %}
   var dataset_{{ dataset }} = {{ items[dataset].get('summary',{}) | tojson() }}
   console.log(dataset_{{ dataset }})
   {%- endfor %}
</script>
</body>