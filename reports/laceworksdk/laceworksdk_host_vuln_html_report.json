            "settings": {
                "template": "./templates/html/vulnerability/host_vulnerability_bootstrap_dataframe.html.j2"
            },
            "reports": [
                {
                    "name": "output",
                    "type": "local_report_handler",
                    "path": "./output/host_vuln_report_2022-01-01-{{ date.strftime('%Y-%m-%d') }}.html"
                }
            ]
