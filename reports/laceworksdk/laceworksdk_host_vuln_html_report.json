{
    "reports": [
        {
            "name": "sample1",
            "datasources": [
                {
                    "name": "vulnerabilities",
                    "type": "laceworksdk_dataset_handler",
                    "organization": false,
                    "subaccount": null,
                    "account": null,
                    "api_key": null,
                    "api_secret": null,
                    "profile": "verato",
